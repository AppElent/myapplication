{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "SEQUELIZE_ENCRYPTION_KEY": {
      "required": true,
      "description": "Random encryption key. Please make your own!",
      "generator": "secret"
    },
    "SETTING": {
      "required": true,
      "value": "HEROKU"
    },
    "FIREBASE_CLIENT_EMAIL": {
      "required": true,
      "description": "Firebase client e-mail"
    },
    "FIREBASE_PRIVATE_KEY": {
      "required": true,
      "description": "Firebase Private key (please make sure to include the ---BEGIN PRIVATE KEY--- and --- END PRIVATE KEY---)"
    },
    "REACT_APP_FIRESTORE_ENVIRONMENT": {
      "required": true,
      "value": "production"
    }
    
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "administratie-app", 
  "description": "Administratie App",
  "repositiry": "https://github.com/ericjansen1988/myapplication",
  "scripts": {
  },
  "stack": "heroku-18"
}
